{
  "name": "parcel-cep-test",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"yarn run start-ui\" \"yarn run start-jsx\"",
    "start-ui": "parcel src/ui/index.ui.html",
    "start-jsx": "parcel watch src/jsx/index.jsx.ts --no-hmr --no-source-maps",
    "build": "yarn run build-ui && yarn run build-jsx",
    "build-ui": "parcel build src/ui/index.ui.html --public-url .",
    "build-jsx": "parcel build src/jsx/index.jsx.ts",
    "zxp": "create-zxp ./dist"
  },
  "dependencies": {
    "fs-extra": "^5.0.0",
    "pino": "^4.10.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.0",
    "@types/pino": "^4.7.1",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "babel-preset-cep": "^0.0.1",
    "babel-preset-extendscript": "^0.0.3",
    "babel-preset-react": "^6.24.1",
    "create-zxp": "^0.0.5",
    "concurrently": "^3.5.1",
    "parcel-bundler": "^1.5.1",
    "parcel-plugin-cep": "link:../parcel-plugin-cep",
    "types-for-adobe": "pravdomil/types-for-adobe",
    "typescript": "^2.7.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
