{
  "name": "react-parcel-cep-starter",
  "version": "1.0.0",
  "cep": {
    "name": "React Parcel CEP Starter",
    "id": "com.mycompany.myextension",
    "hosts": "PPRO",
    "panelWidth": "500",
    "panelHeight": "500",
    "cepVersion": "7.0"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"yarn run start-js\" \"yarn run start-jsx\"",
    "start-js": "parcel src/js/index.html",
    "start-jsx": "parcel watch src/jsx/index.jsx.ts --no-hmr --no-source-maps",
    "build": "yarn run build-js && yarn run build-jsx",
    "build-js": "parcel build src/js/index.html --public-url .",
    "build-jsx": "parcel build src/jsx/index.jsx.ts --no-minify",
    "zxp": "create-zxp ./dist"
  },
  "dependencies": {
    "fs-extra": "^10.0.0",
    "pino": "^6.13.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.0",
    "@types/pino": "^6.3.11",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.3.4",
    "babel-core": "^6.26.3",
    "babel-preset-cep": "^0.0.3",
    "babel-preset-extendscript": "^0.4.0",
    "babel-preset-react": "^6.24.1",
    "cep-interface": "^0.6.0",
    "concurrently": "^6.2.1",
    "create-zxp": "^0.1.0",
    "parcel-bundler": "1.12.3",
    "parcel-plugin-cep": "https://github.com/CEP-Parcel/parcel-plugin-cep",
    "postcss": "^8.0.0",
    "postcss-modules": "^3.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "types-for-adobe": "https://github.com/bbb999/Types-for-Adobe",
    "typescript": "^4.4.3"
  }
}
